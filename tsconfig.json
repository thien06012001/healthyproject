{
  "compilerOptions": {
    "target": "es5",
    "strictNullChecks": false,
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": ["next-env.d.ts","./node_modules/@sanity/base/types/**/*.ts", "**/*.ts", "**/*.tsx", "sanity/schemas/schema.ts"],
  "exclude": ["node_modules"],
  
}
